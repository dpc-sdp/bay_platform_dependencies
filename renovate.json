{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "matchManagers": ["composer"],
      "matchDepTypes": ["require", "require-dev"],
      "groupName": "Composer dependencies"
    }
  ],
  "rangeStrategy": "bump",
  "prConcurrentLimit": 5,
  "labels": ["composer", "dependencies", "automated-pr"]
}